{
    "git": {
      "deploymentEnabled": {
        "main": true
      }
    },
    "functions": {
      "app/api/line/webhook/route.ts": {
        "memory": 1024,
        "maxDuration": 10
      },
      "app/api/lucky-feed/route.ts": {
        "runtime": "edge",
        "maxDuration": 5
      },
      "app/api/debug/status/route.ts": {
        "runtime": "edge",
        "maxDuration": 5
      },
      "app/api/debug/line/route.ts": {
        "memory": 256,
      "maxDuration": 10
      },
      "app/api/debug/users/route.ts": {
        "runtime": "nodejs",
        "maxDuration": 10
      }
    },
    "headers": [
      {
        "source": "/api/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "no-store" }
        ]
      }
    ]
  }